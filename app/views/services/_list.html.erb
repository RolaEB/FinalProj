
  

<div class="row">
<% objects.each do |obj| %>
            <div class="col-lg-4 col-md-6 mb-4">
              <div class="card h-100">
              
              <%=link_to(image_tag(obj.img.thumbIndex.url, alt: 'Photo'),obj.img.url,target:'_blank')  if obj.img? %>
                <div class="card-body">
                  <h4 class="card-title">
                    <%= link_to obj.name ,obj %>
                  </h4>
                  <h5><%= obj.country %> </h5>
                  <h5><%= obj.city %></h5>
                  <p class="card-text"><%= obj.service_category.name %></p>
                </div>
                <div class="card-footer">
                  <small class="text-muted">created <%= obj.created_at.to_date %></small>
                </div>
              </div>
            </div>
            <% end %>
  </div>
  