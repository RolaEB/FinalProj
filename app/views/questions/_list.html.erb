

<% questions.each do |question| %>
    <div class="card mt-4">
      
      <div class="card-body">
        <h3 class="card-title"><%=link_to question.title,question %></h3>
        
        <p class="card-text"><%=question.content %></p>
        <span class="text-info">Posted by <%= question.user.name%> on <%= question.created_at.to_date %> </span>
        
      </div>
    </div>
    <!-- /.card -->
    <% end %>